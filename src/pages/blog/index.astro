---
import Header from "../../components/Header.astro";
import Sidebar from "../../components/Sidebar.astro";
---

<!doctype html>
<html>
  <Header title="Blog | Mohamed Fouda" />
  <body>
    <div class="content">
      <Sidebar selectedItem="blog" />
      <nav class="main">
        <div class="container">
          <main class="fold-content-container">
            <div class="fold-content">
              <h1>Blog</h1>
              <p class="lead">I share here my articles & notes</p>
            </div>
          </main>
          <div class="projects-section">
            <!-- Software Development -->
            <div class="projects-container">
              <div class="single-projects-category">
                <div class="projects-category-image-container">
                  <img
                    src="./../assets/browser.svg"
                    loading="lazy"
                    alt=""
                    class="projects-category-image"
                  />
                </div>
              </div>
              <div class="single-projects-content">
                <h4>Software Development</h4>
                <div class="tech-tools-container">
                  <div class="mini-label-white gray-color">
                    Technologies &amp; tools
                  </div>
                  <div class="chips-container chips-container-small-width">
                    <div class="single-chip small-chip">React</div>
                    <div class="single-chip small-chip">Next.js</div>
                    <div class="single-chip small-chip">Node.js</div>
                    <div class="single-chip small-chip">MongoDB</div>
                    <div class="single-chip small-chip">Heroku</div>
                    <div class="single-chip small-chip">RabbitMQ</div>
                    <div class="single-chip small-chip">Express.js</div>
                    <div class="single-chip small-chip">TypeScript</div>
                    <div class="single-chip small-chip">NestJS</div>
                    <div class="single-chip small-chip">Angular</div>
                    <div class="single-chip small-chip">Postgres</div>
                    <div class="single-chip small-chip">Linode</div>
                  </div>
                </div>
              </div>
            </div>
            <!-- Machine Learning -->
            <div class="projects-container">
              <div class="single-projects-category">
                <div class="projects-category-image-container devops-img">
                  <img
                    src="./../assets/database.svg"
                    loading="lazy"
                    alt="LLM Red Teaming"
                    class="projects-category-image"
                  />
                </div>
              </div>
              <div class="single-projects-content">
                <h4>Machine Learning</h4>
                <div class="tech-tools-container">
                  <div class="mini-label-white gray-color">
                    Technologies &amp; tools
                  </div>
                  <div class="chips-container chips-container-small-width">
                    <div class="single-chip small-chip">Python</div>
                    <div class="single-chip small-chip">Hugging Face</div>
                    <div class="single-chip small-chip">LangChain</div>
                  </div>
                </div>
              </div>
            </div>

            <!-- Leetcode -->
            <div class="projects-container">
              <div class="single-projects-category">
                <div class="projects-category-image-container devops-img">
                  <img
                    src="./../assets/database.svg"
                    loading="lazy"
                    alt="LLM Red Teaming"
                    class="projects-category-image"
                  />
                </div>
              </div>
              <div class="single-projects-content">
                <h4>Leetcode</h4>
                <div class="tech-tools-container">
                  <div class="mini-label-white gray-color">
                    Algorithms & Patterns
                  </div>
                  <div class="chips-container chips-container-small-width">
                    <div class="single-chip small-chip">Two Pointers</div>
                    <div class="single-chip small-chip">Sliding Window</div>
                    <div class="single-chip small-chip">
                      Dynamic Programming
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </nav>
    </div>

    <!--[if lte IE 9]><script src="//cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif]-->
  </body>
</html>
